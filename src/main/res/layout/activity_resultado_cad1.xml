<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:id="@+id/telaReultado"
    android:background="#828282"
    tools:context=".activity.ResultadoCad1Activity">

    <TextView
        android:id="@+id/txtResultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/endResultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"/>

    <TextView
        android:id="@+id/telResultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"/>

    <TextView
        android:id="@+id/emResultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <Button
        android:layout_width="390dp"
        android:layout_height="40dp"
        android:text="OK"
        android:id="@+id/btOk"
        android:onClick="voltaMenu"
        android:background="#0000FF"
        android:textSize="25dp"
        android:layout_marginTop="440dp"/>

    <Button
        android:layout_width="390dp"
        android:layout_height="40dp"
        android:text="ENVIAR"
        android:id="@+id/btsend"
        android:background="@drawable/eml"
        android:textSize="25dp"
        android:layout_marginTop="0dp"/>

    </LinearLayout>


</RelativeLayout>
